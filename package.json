    "@types/pg": "7.14.11",
    "@types/pg-format": "1.0.1",
    "@types/uuid": "8.3.0",
    "cross-spawn": "7.0.3",
    "is-ci": "3.0.0",
    "jest-environment-node": "26.6.2",
    "jest-in-case": "1.0.2",
    "mock-jwks": "0.3.5",
    "nock": "13.0.11",
    "pg-format": "1.0.4",
    "prisma": "2.23.0",
    "vercel": "22.0.1",
    "wait-on": "5.3.0"
  },
  "description": "vercel functions for the giessdenkiez.de project to interact with the postgres database",
  "main": "api/index.ts",
